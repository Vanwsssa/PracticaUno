<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.practicauno.ui.home.HomeFragment"
        android:label="Button and ImageButton"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.practicauno.ui.dashboard.DashboardFragment"
        android:label="TextFields"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.practicauno.ui.notifications.NotificationsFragment"
        android:label="CheckBox, RadioButton and Switch"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_list"
        android:name="com.example.practicauno.ui.list.ListFragment"
        android:label="RecyclerView o ListView"
        tools:layout="@layout/fragment_list" />

    <fragment
        android:id="@+id/navigation_element"
        android:name="com.example.practicauno.ui.element.ElementFragment"
        android:label="TextView, ImageView and ProgressBar"
        tools:layout="@layout/fragment_element" />



</navigation>